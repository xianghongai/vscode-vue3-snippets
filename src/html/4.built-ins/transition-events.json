		"body": [
			"@leave=\"${1:onLeave}\""
		]
	},
	"@after-leave": {
		"prefix": [
			"transition-on-after-leave"
		],
		"body": [
			"@after-leave=\"${1:onAfterLeave}\""
		]
	},
	"@leave-cancelled": {
		"prefix": [
			"transition-on-leave-cancelled"
