{
	"methods property": {
		"prefix": [
			"methods-property",
			"mp"
		],
		"body": [
			"${1:methodName}($2) {",
			"  $3",
			"},$0"
		]
	},
	"methods property, async": {
		"prefix": [
			"methods-property-async",
			"asmp"
		],
		"body": [
			"async ${1:methodName}($2) {",
			"  $3",
			"},$0"
		]
	}
}
